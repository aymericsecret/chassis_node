{
  "name": "chassis_node",
  "version": "1.0.0",
  "description": "Chassis Node",
  "directories": {
    "test": "test"
  },
  "main": "src/index.js",
  "scripts": {
    "coverage": "LOGGER_LEVEL=fatal NODE_ENV=test nyc _mocha",
    "eslint": "eslint src test",
    "lint": "npm run prettier && npm run eslint",
    "mocha": "NODE_ENV=test mocha --bail",
    "prettier": "prettier -l '{src,test}/**/*.{js,yml,yaml,json}'",
    "prettier:write": "prettier --write '{scripts,src,test,migrations}/**/*.{js,json,yaml}'",
    "start": "node src",
    "start:dev": "nodemon --watch src/index.js",
    "test": "npm run lint && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aymericsecret/chassis_node.git"
  },
  "author": "Aymeric Secret",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aymericsecret/chassis_node/issues"
  },
  "homepage": "https://github.com/aymericsecret/chassis_node#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "bunyan-loggly": "^1.4.2",
    "co-express": "^2.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "http-status-codes": "^1.4.0",
    "mongodb": "^3.5.7",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2"
  },
  "engine": {
    "node": "12.16.1"
  }
}
